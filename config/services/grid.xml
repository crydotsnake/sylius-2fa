<?xml version="1.0" encoding="UTF-8" ?>

<container
        xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="BitExpert\SyliusTwoFactorAuthPlugin\Grid\AdminUserGridListener"
                 class="BitExpert\SyliusTwoFactorAuthPlugin\Grid\AdminUserGridListener"
                 public="true">
            <tag name="kernel.event_listener" event="sylius.grid.admin_admin_user" method="addField" />
        </service>
    </services>
</container>
